{
  "name": "bodleian-frontend",
  "version": "0.1.1",
  "scripts": {
    "build": "vite build",
    "dev": "turbo --no-cache start test-watch",
    "format-fix": "prettier --write .",
    "format": "prettier --check .",
    "lint-css-fix": "stylelint --fix '**/*.css'",
    "lint-css": "stylelint -f verbose '**/*.css'",
    "lint-fix": "eslint --fix ./src *js",
    "lint-md": "remark .",
    "lint-md-fix": "remark . --output",
    "lint": "TIMING=1 eslint ./src *js",
    "serve": "vite preview",
    "start": "vite --cors",
    "test": "vitest run --coverage",
    "test-watch": "vitest --coverage",
    "turbo": "turbo build lint lint-css lint-md test format",
    "turbo-clean": "npm run format-fix && turbo lint-fix lint-md-fix lint-css-fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "i18next": "^22.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.20.1",
    "react-transition-group": "^4.4.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@vitest/coverage-v8": "^0.34.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^4.2.0",
    "babel-jest": "^29.3.1",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-recommended": "^6.1.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "turbo": "^1.8.8",
    "vite": "^4.0.5",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.34.6"
  }
}
