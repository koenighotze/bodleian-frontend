{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": [],
      "inputs": ["src/**/*.js"]
    },
    "lint-css": {
      "inputs": ["src/**/*.css"]
    },
    "lint-md": {
      "inputs": ["**/*.md"]
    },
    "lint-fix": {
      "outputs": []
    },
    "test-fix": {
      "outputs": [],
      "inputs": ["src/**/*.js"]
    },
    "lint-css-fix": {
      "inputs": ["src/**/*.css"]
    },
    "lint-md-fix": {
      "inputs": ["**/*.md"]
    }
  }
}
